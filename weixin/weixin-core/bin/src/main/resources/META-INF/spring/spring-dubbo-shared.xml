<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:tx="http://www.springframework.org/schema/tx"
	xmlns:aop="http://www.springframework.org/schema/aop" xmlns:dubbo="http://code.alibabatech.com/schema/dubbo"
	xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd
	http://code.alibabatech.com/schema/dubbo http://code.alibabatech.com/schema/dubbo/dubbo.xsd http://www.springframework.org/schema/tx
            http://www.springframework.org/schema/tx/spring-tx.xsd http://www.springframework.org/schema/aop
            http://www.springframework.org/schema/aop/spring-aop.xsd http://www.springframework.org/schema/context
            http://www.springframework.org/schema/context/spring-context.xsd">

	<description>服务接口定义</description>

	<!-- 缓存服务 -->
	<dubbo:reference id="cacheService" interface="com.baoyuan.cache.CacheService"
		version="${shared.reference.version}" group="${shared.reference.group}"
		init="${shared.reference.init}" timeout="${shared.reference.timeout}"
		retries="${shared.reference.retries}" />

	<!-- 用户管理服务 -->
	<dubbo:reference id="sharedUserService" interface="com.baoyuan.service.UserService"
		version="${shared.reference.version}" group="${shared.reference.group}"
		init="${shared.reference.init}" timeout="${shared.reference.timeout}"
		retries="${shared.reference.retries}" />

	<!-- 角色管理服务 -->
	<dubbo:reference id="sharedRoleService" interface="com.baoyuan.service.RoleService"
		version="${shared.reference.version}" group="${shared.reference.group}"
		init="${shared.reference.init}" timeout="${shared.reference.timeout}"
		retries="${shared.reference.retries}" />

	<!-- 权限管理服务 -->
	<dubbo:reference id="sharedPermissionsService"
		interface="com.baoyuan.service.PermissionsService" version="${shared.reference.version}"
		group="${shared.reference.group}" init="${shared.reference.init}"
		timeout="${shared.reference.timeout}" retries="${shared.reference.retries}" />

	<!-- 租户查询服务 -->
	<dubbo:reference id="sharedTenantsService"
		interface="com.baoyuan.service.TenantsService" version="${shared.reference.version}"
		group="${shared.reference.group}" init="${shared.reference.init}"
		timeout="${shared.reference.timeout}" retries="${shared.reference.retries}" />

	<!-- 应用查询服务 -->
	<dubbo:reference id="sharedApplicationService"
		interface="com.baoyuan.service.ApplicationService" version="${shared.reference.version}"
		group="${shared.reference.group}" init="${shared.reference.init}"
		timeout="${shared.reference.timeout}" retries="${shared.reference.retries}" />

	<!-- 功能管理服务 -->
	<dubbo:reference id="sharedFunctionsService"
		interface="com.baoyuan.service.FunctionsService" version="${shared.reference.version}"
		group="${shared.reference.group}" init="${shared.reference.init}"
		timeout="${shared.reference.timeout}" retries="${shared.reference.retries}" />

</beans>